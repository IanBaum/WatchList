{{yield}}
<div class="container">
  <div class="row">
    {{#each watchList.items as |item|}}
      <div class="col-sm-12">
        <p class="white-text">{{item.name}}</p>
        <img class="product-img" src="{{item.image}}">
        <p class="white-text">{{item.salePrice}}</p>
      </div>
    {{/each}}
  </div>
</div>
