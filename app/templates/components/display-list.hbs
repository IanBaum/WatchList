{{yield}}
<div class="container">
  <div class="row">
    {{#each watchList.items as |item|}}
      <div class="col-sm-12 resultPanel">
        <p>{{item.name}}</p>
        <img class="product-img" src="{{item.image}}">
        <p>Regular Price: ${{item.regularPrice}}</p>
        <button class="btn btn-danger" {{action 'deleteProduct' item}}>Remove</button><br>
      </div>
    {{/each}}
  </div>
</div>
